{
  "name": "finance-page",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "Elite-skills",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w server\" \"npm run dev -w Elite-skills\"",
    "dev:client": "npm run dev -w Elite-skills",
